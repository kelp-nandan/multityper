{
  "name": "server",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "format": "npx prettier --write \"src/**/*.ts\" \"migrations/**/*.js\"",
    "format:check": "npx prettier --check \"src/**/*.ts\" \"migrations/**/*.js\"",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:migrate:undo": "npx sequelize-cli db:migrate:undo",
    "db:migrate:undo:all": "npx sequelize-cli db:migrate:undo:all",
    "db:migrate:status": "npx sequelize-cli db:migrate:status",
    "db:reset": "npm run db:migrate:undo:all && npm run db:migrate",
    "db:fresh": "npm run db:reset",
    "db:setup": "npm run db:migrate && echo 'Database setup complete! Ready to use.'"
  },
  "dependencies": {
    "@nestjs/common": "11.0.1",
    "@nestjs/config": "4.0.2",
    "@nestjs/core": "11.0.1",
    "@nestjs/jwt": "11.0.2",
    "@nestjs/passport": "11.0.5",
    "@nestjs/platform-express": "11.0.1",
    "@nestjs/platform-socket.io": "11.0.1",
    "@nestjs/sequelize": "11.0.1",
    "@nestjs/websockets": "11.0.1",
    "bcrypt": "6.0.0",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.3",
    "cookie-parser": "1.4.7",
    "passport": "0.7.0",
    "passport-jwt": "4.0.1",
    "pg": "8.16.3",
    "pg-hstore": "2.3.4",
    "redis": "5.8.2",
    "reflect-metadata": "0.2.2",
    "rxjs": "7.8.1",
    "sequelize": "6.37.7",
    "sequelize-typescript": "2.1.6",
    "socket.io": "4.8.1"
  },
  "devDependencies": {
    "@nestjs/cli": "11.0.0",
    "@nestjs/schematics": "11.0.0",
    "@types/bcrypt": "6.0.0",
    "@types/express": "5.0.0",
    "@types/node": "22.10.7",
    "@types/passport-jwt": "4.0.1",
    "@types/pg": "8.16.0",
    "@types/sequelize": "4.28.20",
    "prettier": "3.4.2",
    "sequelize-cli": "6.6.3",
    "source-map-support": "0.5.21",
    "ts-loader": "9.5.2",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.7.3"
  }
}